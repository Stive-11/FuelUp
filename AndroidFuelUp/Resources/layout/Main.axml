<?xml version="1.0" encoding="utf-8"?>
<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
    android:orientation="vertical"
    android:layout_width="match_parent"
    android:layout_height="match_parent">
    <LinearLayout
        android:orientation="horizontal"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:id="@+id/linearLayout1"
        android:layout_gravity="left">
        <Button
            android:id="@+id/MyButton"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:text="@string/MainDownload"
            android:layout_marginRight="0.0dp" />
        <Button
            android:text="Hi"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:id="@+id/HiButton" />
        <Button
            android:text="@string/Request"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:id="@+id/menuButton" />
        <Button
            android:text="@string/testMenuButton"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:id="@+id/testMenuButton" />
    </LinearLayout>
    <LinearLayout
        android:orientation="vertical"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:id="@+id/linearLayoutStartFinish"
        android:layout_gravity="left">
        <EditText
            android:id="@+id/edittextStartPoint"
            android:layout_width="fill_parent"
            android:layout_height="fill_parent"
            android:singleLine="true" />
        <EditText
            android:id="@+id/edittextFinishPoint"
            android:layout_width="fill_parent"
            android:layout_height="fill_parent"
            android:singleLine="true" />
    </LinearLayout>
    <FrameLayout
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        android:id="@+id/frameLayout1">
        <fragment
            android:id="@+id/map"
            android:layout_width="match_parent"
            android:layout_height="match_parent"
            android:name="com.google.android.gms.maps.MapFragment" />
    </FrameLayout>
</LinearLayout>